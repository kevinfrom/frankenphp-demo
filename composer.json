{
    "name": "vjm/frankenphp-demo",
    "description": "A demo application for FrankenPHP",
    "version": "0.1.0",
    "license": "MIT",
    "type": "project",
    "scripts": {
        "lint": "phpcs",
        "lint:fix": "phpcbf"
    },
    "require": {
        "php": "^8.4",
        "psr/container": "^2.0.2",
        "psr/clock": "^1.0"
    },
    "autoload": {
        "files": [
            "core/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4.23",
        "squizlabs/php_codesniffer": "^3.12.2"
    }
}
